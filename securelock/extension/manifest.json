{
  "name": "ExamShield Security Monitor",
  "version": "1.0.0",
  "manifest_version": 3,
  "key": "",
  "description": "ExamShield - Real-time security monitoring for secure testing environments",
  "permissions": ["nativeMessaging", "tabs", "activeTab"],
  "host_permissions": ["http://localhost:3000/*"],
  "background": { 
    "service_worker": "bg.js"
  },
  "content_scripts": [{
    "matches": ["http://localhost:3000/*"],
    "js": ["content.js"],
    "run_at": "document_start"
  }]
}